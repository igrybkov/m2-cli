#!/usr/bin/env php
<?php

$container = require_once __DIR__ . '/../etc/bootstrap.php';

// Init App with name and version
/** @var \Ahc\Cli\Application $app */
$app = $container->get(\Ahc\Cli\Application::class);

/** @var \Ahc\Cli\Input\Command[] $commands */
$commands = $container->get('commands');
foreach ($commands as $alias => $command) {
    $app->add($command, is_string($alias) ? $alias : '');
}

$app->handle($_SERVER['argv']);
